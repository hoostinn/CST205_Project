#infoPanel {
    background-color: #F4F4F4;
    border-radius: 25px;
}

#mapFrame {
    background-color: white;
    border-radius: 25px;
}

#searchBox {
    border-radius: 20px;
    border: 2px solid #999;
    padding-left: 15px;
    font-size: 18px;
    background: #FFFFFF;
}

#searchBtn, #menuBtn {
    border-radius: 22px;
    background-color: #E0E0E0;
    font-size: 22px;
    border: none;
}
